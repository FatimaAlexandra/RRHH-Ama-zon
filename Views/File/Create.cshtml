
<link rel="stylesheet" href="ruta-a-tu-archivo/sweetalert2.min.css">
<script src="ruta-a-tu-archivo/sweetalert2.min.js"></script>

<main class="mt-2">

<div class="container">
    <h3 class="mt-4 mb-3">Cargar archivo de empleados</h3>
    <form asp-action="CargarArchivo" asp-controller="File" method="post" enctype="multipart/form-data">
        <div class="form-group">
            <label for="archivo">Seleccionar archivo:</label>
            <input type="file" class="form-control-file" name="archivo" id="archivo" />
        </div><br />
        <button type="submit" class="btn btn-primary">Cargar archivo</button>
    </form>
</div>
</main>
@if (TempData["successMessage"] != null)
{
    <script>
        Swal.fire({
            title: '¡Éxito!',
            text: '@TempData["successMessage"]',
            icon: 'success',
            confirmButtonText: 'Aceptar'
        });
    </script>
}